        </activity>

        <activity android:name=".demo_a.First_Mediaplayer">
        </activity>
        <activity android:name=".demo_b.DemoBActivity">
        </activity>
        <activity android:name=".demo_c.DemoCActivity">
        </activity>
        <activity android:name=".demo_d.DemoDActivity">
        </activity>
        <activity android:name=".demo_e.DemoEActivity">
        </activity>

    </application>

</manifest>